{
  "name": "language-repository",
  "description": "Language Repository Frontend",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --sourcemap --extractCss --ssl 1 --ssl-key /etc/ssl/private/server-config/localhost.selfsigned.key --ssl-cert /etc/ssl/certs/server-config/localhost.selfsigned.crt",
    "hmr": "ng serve --hmr --environment=hmr --ssl 1 --ssl-key /etc/ssl/private/localhost.selfsigned.key --ssl-cert /etc/ssl/certs/localhost.selfsigned.crt",
    "vbox": "ng serve --environment=vbox --ssl 1 --ssl-key /etc/ssl/private/localhost.selfsigned.key --ssl-cert /etc/ssl/certs/localhost.selfsigned.crt  --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "compass-mixins": "^0.12.10",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
